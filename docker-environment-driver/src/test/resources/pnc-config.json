{
  "@class":"ModuleConfigJson","name":"pnc-config",
  "configs":[
      {
      "@module-group":"pnc",
      "configs": [
          {
            "@module-config":"docker-environment-driver",
            "ip":"${env.PNC_DOCKER_IP}",
            "inContainerUser":"${env.PNC_DOCKER_CONT_USER}",
            "inContainerUserPassword":"${env.PNC_DOCKER_CONT_PASSWORD}",
            "dockerImageId":"${env.PNC_DOCKER_IMAGE_ID}",
            "proxyServer":"${env.PNC_DOCKER_PROXY_SERVER}",
            "proxyPort":"${env.PNC_DOCKER_PROXY_PORT}",
            "nonProxyHosts":"${env.PNC_DOCKER_NON_PROXY_HOSTS}",
            "firewallAllowedDestinations":"${env.PNC_DOCKER_IMAGE_FIREWALL_ALLOWED}"
          }
        ]
      }
  ]
}

